public class Main
{
  public static void main(String[] args)
  {
    int arr[] = {0,22,3233,24,35,45,456,4,6,89};     
    int max = arr[0];
    int min = arr[0];
    for(int i = 1; i < arr.length; i++)
    {
      if(max < arr[i])
      {
        max = arr[i];
      }
      else if(min > arr[i])
      {
        min = arr[i];
      }              
    }
    System.out.println("Maximum number = " + max );
    System.out.println("Minimum number = " + min );
  }
}
